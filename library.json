    "repository": {
      "type": "git",
      "url": "https://github.com/username/SignalAnalysis.git"
    },
    "authors": [
      {
        "name": "Seidel Christof",
        "email": "SeidC2221@gmail.com"
      }
    ],
    "frameworks": "*",
    "platforms": "*",
    "license": "MIT",
